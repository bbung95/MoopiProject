<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.moopi.mvc.service.followLike.FollowLikeDao">   	
 	
 	
 	
 	
	<select id ="getLike" parameterType ="followLike" resultType="int">
	SELECT
	 COUNT(*)
	FROM
	  FOLLOW_LIKE
	WHERE
	  USER_ID   = #{followLike.user.userId)
	AND
	  FL_TYPE   = #{followLike.flType}
	AND
	  FL_TARGET = #{followLike.flTarget}
	</select>
	
